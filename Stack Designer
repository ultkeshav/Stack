stack<int> push(vector<int> &arr) {
    int num = arr.size();
    
    stack<int> s;
    
    for(int i=0; i<num ; i++){
        
        s.push(arr[i]);
    }
    return s;
}

/* _pop function to print elements of the stack
   remove as well
*/
void pop(stack<int> s) {
    
    while(!s.empty()){
        
        cout<<s.top()<<" ";
        s.pop();
    }
}
